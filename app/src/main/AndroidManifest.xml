<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="swu.lostfindapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".IntroActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
        <activity
            android:name=".WriteActivity" android:label="글쓰기" />
        <activity android:name=".LostActivity" android:label="찾아요"/>
        <activity
            android:name=".WriteFindActivity" android:label="글쓰기" />
        <activity android:name=".FindActivity" android:label="있어요"/>
        <activity android:name=".DisuseActivity" android:label="폐기직전"/>
        <activity android:name=".LoginActivity" android:label="로그인"/>
        <activity android:name=".JoinActivity" android:label="회원가입"/>
        <activity android:name=".SettingActivity" android:label="설정"/>
        <activity android:name=".NoticeActivity" android:label="공지"/>
        <activity android:name=".NoticeWriteActivity" android:label="공지"/>
        <activity android:name=".InstructionsActivity" android:label="앱 사용방법"></activity>
        <activity android:name=".QuestionActivity" android:label="문의하기"></activity>
        <activity android:name=".SearchListActivity" android:label="검색결과"></activity>



    </application>

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.Camera" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

</manifest>